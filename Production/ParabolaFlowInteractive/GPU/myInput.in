BEGIN INFORMATION
  TITLE = "DIRECT NUMERICAL SIMULATION OF PARABOLA FLOW via LINEAR SYSTEM OF EQUATIONS"
END INFORMATION

BEGIN GEOMETRY_DEFINITION
  NX = 201
  NY = 801
  XMIN = -20
  XMAX = 20
  YMIN = 1
  YMAX = 11
END GEOMETRY

BEGIN STEP 1
  TITLE = "FLOW DEVELOPMENT"
  BEGIN SOLUTION_CONTROL
    BEGIN LINEAR_SOLVE
      relative tolerance = 1e-5
      solver = iterative
    END LINEAR_SOLVE

    BEGIN TIME_CONTROL
      time end = 1
      max time control = REYNOLDS
      max time scale = 0.1
    END TIME_CONTROL
  END SOLUTION_CONTROL
END STEP 1

BEGIN FLOW_DEFINITION
  BEGIN REYNOLDS
    function = constant
    value = 700
    scale factor = 1
  END REYNOLDS

  BEGIN CIRCULATION
    function = constant
    value = 2
    scale factor = 1
  END CIRCULATION
END FLOW_DEFINITION

BEGIN RESULT
  name = SurfaceVelocity
  description = "Velocity History Along Parabola Surface"
  active step = 1
  output initial step = true
  output final step = true
  time increment = 0.1
  buffer iterations = 100
  variable = pU
  iX = 1:NX
  jY = 2
END RESULT

BEGIN RESULT
  name = U-VelocityProfile
  description = "U-Velocity Profile History at Parabola Center Parabola Surface"
  active step = 1
  output initial step = true
  output final step = true
  time increment = 0.1
  buffer iterations = 100
  variable = pU
  iX = ceil(NX/2)
  jY = 1:2*IBL
END RESULT

BEGIN RESULT
  name = V-VelocityProfile
  description = "U-Velocity Profile History at Parabola Center Parabola Surface"
  active step = 1
  output initial step = true
  output final step = true
  time increment = 0.1
  buffer iterations = 100
  variable = pV
  iX = ceil(NX/2)
  jY = 1:2*IBL
END RESULT


BEGIN LOG_INFO
  name = pfiSIM.log
  time increment = 0.1
  initial step = true
  final step = true
  log data = tstep, t, tElapsed
END LOG_INFO

BEGIN IN_SITU
END IN_SITU
